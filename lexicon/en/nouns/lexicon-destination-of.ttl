@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :destination_of ;
  lemon:entry    :of .

:destination_of a          lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :destination_form ;
  lemon:synBehavior    :destination_of_nounpp ;
  lemon:sense          :destination_sense1_ontomap ;
  lemon:sense          :destination_sense2_ontomap .

:destination_form a    lemon:Form ;
  lemon:writtenRep "destination"@en .

:destination_of_nounpp a           lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:destination_sense1_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping      :destination_sense1_ontomap ;
  lemon:reference        <http://dbpedia.org/ontology/destination> ;
  lemon:subjOfProp       :arg2 ;
  lemon:objOfProp        :arg1 ;
  lemon:condition        :destination_condition_publictransitsystem .

:destination_condition_publictransitsystem a   lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/PublicTransitSystem> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/PopulatedPlace> .

:destination_sense2_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping      :destination_sense2_ontomap ;
  lemon:reference        <http://dbpedia.org/ontology/destination> ;
  lemon:subjOfProp       :arg2 ;
  lemon:objOfProp        :arg1 ;
  lemon:condition        :destination_condition_airplane .

:destination_condition_airplane a   lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Airplane> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .